// Look at result of testcase, guess the code!




function testit(s){
  return [...s].map((el,i)=>(n=>/[a-z]/i.test(el)?String.fromCharCode((el.charCodeAt(0)-(n?39:71)+(-1)**(i%2))%26+(n ? 65 : 97)):el)(el.toUpperCase()==el)).join("");
}
