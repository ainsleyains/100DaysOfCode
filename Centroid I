// Write a function centroid(c) to calculate the centroid of 3D coordinates.

// Return the results as a list of floats. Round the values to two decimal places.



function centroid(c) {
  const x = c.map((el,i)=> (c.reduce((a,c)=>a + c[0],0)/c.length).toFixed(2)*1)[0];
  const y = c.map((el,i)=> (c.reduce((a,c)=>a + c[1],0)/c.length).toFixed(2)*1)[0];
  const z = c.map((el,i)=> (c.reduce((a,c)=>a + c[2],0)/c.length).toFixed(2)*1)[0];
  return [x, y, z];
}

